# Sample environment variables for the Web Search Sourcing Agent
# Copy this file to .env and fill in your actual API keys and configuration

# API Keys
GOOGLE_API_KEY=your-google-api-key
GOOGLE_CSE_ID=your-custom-search-engine-id
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key-starting-with-sk-ant

# LLM Configuration
# Options: openai, anthropic, ollama, llama
LLM_TYPE=openai

# Ollama Configuration (if using Ollama or Llama)
OLLAMA_API_URL=http://localhost:11434
OLLAMA_MODEL_NAME=llama3.2

# Search Configuration
REQUEST_DELAY=1.0
MIN_RESULTS=10

# Azure Configuration (if using Azure Key Vault)
USE_KEY_VAULT=false
KEY_VAULT_URL=https://your-key-vault-name.vault.azure.net/

# Application Insights (for Azure telemetry)
APPLICATION_INSIGHTS_CONNECTION_STRING=

# Logging
LOG_LEVEL=INFO